<div class="member" id="member_<%=member.id%>">
   <%if @current_member and (@current_member==member or @current_member.is_phi)%>
      <div style="float:right">
         <%= link_to 'Edit', edit_member_path(member) %>
         <%= link_to 'Destroy', member, :confirm => 'Are you sure you want to delete this member?', :method => :delete %>
      </div>
   <%end%>
   
   <%= link_to member.name, member %>
   
   
   
</div>